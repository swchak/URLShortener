if='this.shortUrl.length>0'\n              :value=\"shortUrl\"\n              label=\"Shortened Url\"\n              filled\n              disabled\n            ></v-text-field>\n    \n    </div>\n</template>\n\n<script>\nimport { minLength, maxLength, url} from 'vuelidate/lib/validators'\nimport httpSvc from '../services'\n\nexport default {\n    name: \"ShortenUrlPage\",\n    data() {\n        return {\n             longUrl: \"\",\n             shortUrl: \"\",\n             shortCode: \"\"\n        }\n    },\n    validations: {\n        longUrl: {\n            minLength: minLength(1),\n            maxLength: maxLength(200),\n            url\n        }\n    },\n    methods: {\n        submit() {\n            console.log(this.longUrl)\n            \n            httpSvc.post('/shorten-url', {\n                longUrl: this.longUrl\n            }).then(res => {\n                this.shortUrl = 'http://localhost:8080/' + res.data.shortUrlCode\n                this.shortCode = res.data.shortUrlCode\n                console.log(res.config)\n            })\n            // call post api\n            \n        }\n    }\n}\n</script>"]}]}