elidate/lib/validators'\nimport { httpSvc } from '../services'\n\nexport default {\n    name: \"ShortenUrlPage\",\n    data() {\n        return {\n             longUrl: \"\"\n        }\n    },\n    validations: {\n        longUrl: {\n            minLength: minLength(1),\n            maxLength: maxLength(200),\n            url\n        }\n    },\n    methods: {\n        submit() {\n            console.log(this.longUrl)\n            \n            httpSvc.post('/shorten-url', {\n                longUrl: this.longUrl\n            }).then(res => console.log(res))\n            // call post api\n            \n        }\n    }\n}\n</script>"]}]}